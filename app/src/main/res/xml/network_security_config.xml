<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Permet l'accès HTTP à un serveur spécifique, comme un serveur ngrok -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="false">172.20.10.5</domain>
        <domain includeSubdomains="true">10.0.2.2</domain>
        <domain includeSubdomains="true">192.168.100.179</domain>
    </domain-config>
</network-security-config>
